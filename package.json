{
  "name": "babel-boilerplate-module",
  "version": "0.0.1",
  "description": "Model for all authentication of user",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidlondono/babel-boilerplate-module"
  },
  "scripts": {
    "pretest": "npm run lint || true",
    "test": "NODE_ENV=test mocha -u bdd -R spec --require babel-core/register",
    "clean": "rimraf lib && mkdir lib",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "build": "npm run clean && npm run build:module",
    "build:module": "babel -d ./lib ./src -s",
    "postinstall": "npm run build",
    "watch:babel": "babel ./src --watch --out-dir ./lib",
    "commit": "git add . && git-cz"
  },
  "author": "David Alejandro Londo√±o",
  "license": "MIT",
  "dependencies": {
    "babel-core": "6.17.0",
    "babel-preset-es2015-node5": "1.2.0",
    "babel-preset-stage-3": "6.17.0",
    "rimraf": "2.5.4"
  },
  "devDependencies": {
    "arrow-mocha": "^1.0.1",
    "babel-eslint": "6.0.4",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "commitizen": "2.8.6",
    "cz-conventional-changelog": "1.2.0",
    "dirty-chai": "1.2.2",
    "eslint": "2.10.2",
    "eslint-config-airbnb-base": "3.0.1",
    "eslint-plugin-import": "1.8.0",
    "eslint-plugin-mocha": "4.5.1",
    "mocha": "3.0.2",
    "sinon": "1.17.5",
    "sinon-as-promised": "4.0.2"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
}
